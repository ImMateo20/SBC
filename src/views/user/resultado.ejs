<%- include('../partials/header.ejs') %>
    <%- include('../partials/navbar.ejs') %>


        <main class="main-principal">

            <div class="titulo-resultados">
                <h2>Resultados</h2>
            </div>

            <div class="div-resultados">
                <div class="div-resultados-recomendaciones">
                    <div class="div-resultado-diagnostico">
                        <h4>Diagnostico</h4>
                        <img src='<%= diagnostico.imagen %>'' alt="">
                        <p>
                            <%= diagnostico.diagnostico %>
                        </p>
                    </div>
                    <% if (diagnostico.informacion) { %>
                     
                        <br><br>
                        <div class="div-resultado-informacion">
                            <h4>Acerca de</h4>
                            <p>
                                <%= diagnostico.informacion%>
                            </p>
                        </div>
                        <% } %>
                    <br><br>
                    <div class="div-resultado-recomendacion">
                        <h4>Recomendaciones</h4>
                        <p>
                            <%= diagnostico.recomendacion %>
                        </p>
                    </div>
                </div>
                <a href="/"><button class="btn btn-secondary">Volver al inicio</button></a>
            </div>

        </main>


        <%- include('../partials/footer.ejs') %>
